


<html class="no-js">
<head>
  
  
  
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <meta name="format-detection" content="telephone=no"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta charset="utf-8">
  <meta name="renderer" content="webkit">
<script>
(function() {
	var url=window.location.href;	
	if(url.substr(url.substr(7).indexOf("/")+7)=='/index.html' || url.substr(url.substr(7).indexOf("/")+7)=='/'||url.substr(url.substr(7).indexOf("/")+7)=='/opencms/opencms/index.html'||url.substr(url.substr(7).indexOf("/")+7)=='/opencms/opencms/') {
		var sUserAgent = navigator.userAgent.toLowerCase();
		var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
		var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
		var bIsMidp = sUserAgent.match(/midp/i) == "midp";
		var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
		var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
		var bIsAndroid = sUserAgent.match(/android/i) == "android";
		var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
		var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
		var bIsIE = sUserAgent.match(/iemobile/i) == "iemobile";
		if (bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM||bIsIE) {
		
			window.location.replace('mobile/cn/sc/');
		}else{
			window.location.replace('cn/sc/');
		}
	
	}
})();
</script>
</head>
</html>